{
  "name": "quoteme-monorepo",
  "version": "1.0.0",
  "description": "QuoteMe Full-Stack Monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev -w apps/api\" \"npm run dev -w apps/web\"",
    "build": "npm run build --workspaces",
    "db:generate": "npm run generate -w packages/database",
    "db:push": "npm run push -w packages/database"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "concurrently": "^9.2.1",
    "jsonwebtoken": "^9.0.3"
  }
}
