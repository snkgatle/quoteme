{
  "name": "quoteme-monorepo",
  "version": "1.0.0",
  "description": "QuoteMe Full-Stack Monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev -w apps/api & npm run dev -w apps/web",
    "build": "npm run build --workspaces",
    "db:generate": "npm run generate -w packages/database",
    "db:push": "npm run push -w packages/database"
  }
}
